(lambda (exchange)	(let* (		(this-session (get-current-session exchange))		(parameters (get-exchange-parameters exchange))		(language (@key parameters 'Language))		(interface (@key parameters 'Interface)))		(if (invalid? language)			(set! language "English"))		(if (invalid? interface)			(set! interface "Graphics"))		(set! (@key this-session 'language) language)	;;; save in the session		(set! (@key this-session 'interface) interface)		(set! (@key this-session (get-exchange-url exchange)) nil)		(+ "Language is " language " and interface is " interface)		exchange))